{
  "name": "maestro-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Claude Maestro process management",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && node build.mjs",
    "build:types": "tsc",
    "build:bundle": "node build.mjs",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/bundle.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["mcp", "claude", "maestro"],
  "author": "Jack Wakem",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
